#include "holberton.h"


void free_listint(est_env *head)
{
        est_env *thisnode = head, *nextnode;

        while (thisnode)
        {
                nextnode = thisnode->next;
		free(thisnode->envar);
		free(thisnode->value);

                free(thisnode);

                thisnode = nextnode;
        }


}
